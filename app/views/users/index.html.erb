<h1>Listing users</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Email</th>
    <th>Name</th>
    <th>Firstname</th>
    <th>Birthday</th>
    <th>Password</th>
    <th>Privacy setting</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%= user.email %></td>
    <td><%= user.name %></td>
    <td><%= user.firstname %></td>
    <td><%= user.birthday %></td>
    <td><%= user.password %></td>
    <td><%= user.privacy_setting %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
